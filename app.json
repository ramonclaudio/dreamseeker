{
  "expo": {
    "name": "DreamSeeker",
    "slug": "dreamseeker",
    "description": "From dreaming to doing. Set ambitious goals, break them into daily micro-actions, and crush milestones with streaks, XP, and celebrations.",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "dreamseeker",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rmncldyo.dreamseeker",
      "appleTeamId": "SWH9LXWCC3",
      "buildNumber": "1",
      "icon": "./assets/images/dreamseeker-app-icon.icon",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to take profile photos.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to select profile photos.",
        "NSUserNotificationsUsageDescription": "DreamSeeker sends reminders for your daily actions, streak milestones, and progress updates.",
        "aps-environment": "production"
      },
      "associatedDomains": [
        "applinks:usable-sardine-855.convex.site"
      ],
      "privacyManifests": {
        "NSPrivacyAccessedAPITypes": [
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryUserDefaults",
            "NSPrivacyAccessedAPITypeReasons": [
              "CA92.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryFileTimestamp",
            "NSPrivacyAccessedAPITypeReasons": [
              "0A2A.1",
              "3B52.1",
              "C617.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategoryDiskSpace",
            "NSPrivacyAccessedAPITypeReasons": [
              "E174.1",
              "85F4.1"
            ]
          },
          {
            "NSPrivacyAccessedAPIType": "NSPrivacyAccessedAPICategorySystemBootTime",
            "NSPrivacyAccessedAPITypeReasons": [
              "35F9.1"
            ]
          }
        ]
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "asyncRoutes": {
            "default": "development"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#E07B4F",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "backgroundColor": "#1A1614"
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#3b82f6",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "./plugins/with-auto-signing",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "27495040-f0ab-4f2f-b207-72a0dbc118b6"
      },
      "privacyPolicyUrl": "https://ramonclaudio.com/apps/dreamseeker/privacy",
      "termsOfServiceUrl": "https://ramonclaudio.com/apps/dreamseeker/terms"
    },
    "owner": "ramonclaudio",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/27495040-f0ab-4f2f-b207-72a0dbc118b6"
    },
    "android": {
      "package": "com.rmncldyo.dreamseeker"
    }
  }
}
