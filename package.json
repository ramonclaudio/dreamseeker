{
  "name": "dreamseeker",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "patch-package && (test -f .env.local || cp .env.example .env.local 2>/dev/null || true)",
    "start": "expo start --dev-client --clear",
    "env:dev": "cp .env.local.dev .env.local && echo 'Switched to DEV'",
    "env:prod": "cp .env.local.prod .env.local && echo 'Switched to PROD'",
    "convex:dev": "convex dev",
    "convex:deploy": "convex deploy",
    "ios": "npm run clean:metro && expo prebuild --clean --platform ios && expo run:ios --no-build-cache",
    "start:clean": "npm run clean:nuclear && npx convex codegen && npm run ios",
    "ios:device": "npm run clean:metro && expo prebuild --clean --platform ios && expo run:ios --device --no-build-cache",
    "prebuild": "expo prebuild --clean --platform ios",
    "clean": "rm -rf node_modules ios .expo dist coverage convex/_generated $TMPDIR/metro-* $TMPDIR/haste-map-* $TMPDIR/react-* ~/Library/Developer/Xcode/DerivedData/dreamseeker-* && npm install",
    "clean:nuclear": "rm -rf node_modules ios .expo dist coverage convex/_generated $TMPDIR/metro-* $TMPDIR/haste-map-* $TMPDIR/react-* $TMPDIR/expo-* ~/Library/Developer/Xcode/DerivedData/dreamseeker-* ~/Library/Caches/CocoaPods ~/.expo/ios-simulator-app-cache ~/.expo/native-modules-cache ~/.expo/schema-cache && npm install",
    "clean:metro": "rm -rf $TMPDIR/metro-* $TMPDIR/haste-map-*",
    "clean:xcode": "rm -rf ~/Library/Developer/Xcode/DerivedData/dreamseeker-*",
    "check": "npm run lint && npm run typecheck",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "analyze": "EXPO_ATLAS=true npx expo export",
    "analyze:ios": "EXPO_ATLAS=true npx expo export -p ios",
    "seed": "npx convex run --no-push seed:clearAndReseed"
  },
  "dependencies": {
    "@better-auth/expo": ">=1.4.15 <1.5.0",
    "@convex-dev/better-auth": "^0.10.10",
    "@convex-dev/resend": "^0.2.3",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.6.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.28",
    "better-auth": ">=1.4.15 <1.5.0",
    "convex": "^1.31.7",
    "convex-helpers": "^0.1.111",
    "convex-revenuecat": "^0.1.6",
    "expo": "55.0.0-canary-20260128-67ce8d5",
    "expo-apple-authentication": "55.0.4-canary-20260128-67ce8d5",
    "expo-application": "55.0.4-canary-20260128-67ce8d5",
    "expo-clipboard": "55.0.4-canary-20260128-67ce8d5",
    "expo-constants": "55.0.3-canary-20260128-67ce8d5",
    "expo-dev-client": "55.0.4-canary-20260128-67ce8d5",
    "expo-device": "55.0.4-canary-20260128-67ce8d5",
    "expo-font": "55.0.4-canary-20260128-67ce8d5",
    "expo-glass-effect": "55.0.4-canary-20260128-67ce8d5",
    "expo-haptics": "55.0.4-canary-20260128-67ce8d5",
    "expo-image": "55.0.4-canary-20260128-67ce8d5",
    "expo-image-picker": "55.0.4-canary-20260128-67ce8d5",
    "expo-linking": "55.0.4-canary-20260128-67ce8d5",
    "expo-network": "55.0.4-canary-20260128-67ce8d5",
    "expo-notifications": "55.0.5-canary-20260128-67ce8d5",
    "expo-router": "55.0.0-canary-20260128-67ce8d5",
    "expo-secure-store": "55.0.4-canary-20260128-67ce8d5",
    "expo-sharing": "55.0.5-canary-20260128-67ce8d5",
    "expo-splash-screen": "55.0.4-canary-20260128-67ce8d5",
    "expo-status-bar": "55.0.3-canary-20260128-67ce8d5",
    "expo-symbols": "55.0.4-canary-20260128-67ce8d5",
    "expo-updates": "55.0.6-canary-20260128-67ce8d5",
    "expo-web-browser": "55.0.4-canary-20260128-67ce8d5",
    "react": "19.2.0",
    "react-native": "0.83.1",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-keyboard-controller": "1.20.4",
    "react-native-purchases": "^9.7.6",
    "react-native-purchases-ui": "^9.7.6",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.20.0",
    "react-native-svg": "15.15.1",
    "react-native-view-shot": "^4.0.3",
    "react-native-worklets": "0.7.2"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.2.10",
    "eas-build-cache-provider": "^16.4.2",
    "eslint": "^9.39.2",
    "eslint-config-expo": "55.0.1-canary-20260128-67ce8d5",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "expo-atlas": "^0.4.3",
    "jest": "^30.2.0",
    "jest-expo": "55.0.0-canary-20260128-67ce8d5",
    "patch-package": "^8.0.1",
    "react-dom": "19.2.0",
    "react-test-renderer": "^19.2.4",
    "typescript": "~5.9.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "overrides": {
    "@convex-dev/better-auth": {
      "better-auth": ">=1.4.15 <1.5.0"
    }
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  }
}
