{
  "name": "expo-starter-app",
  "main": "expo-router/entry",
  "version": "1.0.0-alpha-1",
  "scripts": {
    "postinstall": "patch-package && (test -f .env.local || cp .env.local.dev .env.local 2>/dev/null || true)",
    "eas-build-post-install": "npx convex codegen",
    "start": "expo start --dev-client --clear",
    "env:dev": "cp .env.local.dev .env.local && echo 'Switched to DEV'",
    "env:prod": "cp .env.local.prod .env.local && echo 'Switched to PROD'",
    "convex": "convex dev",
    "convex:deploy": "convex deploy",
    "ios": "npm run clean:metro && expo prebuild --clean && expo run:ios --no-build-cache",
    "ios:device": "npm run clean:metro && expo prebuild --clean && expo run:ios --device --no-build-cache",
    "android": "npm run clean:metro && expo prebuild --clean && expo run:android --no-build-cache",
    "android:device": "npm run clean:metro && expo prebuild --clean && expo run:android --device --no-build-cache",
    "web": "expo start --web --clear",
    "prebuild": "expo prebuild --clean",
    "clean": "rm -rf node_modules ios android .expo dist coverage convex/_generated $TMPDIR/metro-* $TMPDIR/haste-map-* $TMPDIR/react-* ~/Library/Developer/Xcode/DerivedData/expostarterapp-* && npm install && expo prebuild",
    "clean:nuclear": "rm -rf node_modules ios android .expo dist coverage convex/_generated $TMPDIR/metro-* $TMPDIR/haste-map-* $TMPDIR/react-* ~/Library/Developer/Xcode/DerivedData/expostarterapp-* ~/.expo/ios-simulator-app-cache ~/.expo/native-modules-cache ~/.expo/schema-cache ~/.gradle/caches && npm install && expo prebuild",
    "clean:metro": "rm -rf $TMPDIR/metro-* $TMPDIR/haste-map-*",
    "clean:xcode": "rm -rf ~/Library/Developer/Xcode/DerivedData/expostarterapp-*",
    "check": "npm run lint && npm run typecheck",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "analyze": "EXPO_ATLAS=true npx expo export",
    "analyze:ios": "EXPO_ATLAS=true npx expo export -p ios",
    "analyze:android": "EXPO_ATLAS=true npx expo export -p android",
    "analyze:web": "EXPO_ATLAS=true npx expo export -p web"
  },
  "dependencies": {
    "@better-auth/expo": "^1.4.15",
    "@convex-dev/better-auth": "^0.10.10",
    "@convex-dev/resend": "^0.2.3",
    "@convex-dev/stripe": "^0.1.3",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.10.1",
    "@react-navigation/elements": "^2.9.5",
    "@react-navigation/native": "^7.1.28",
    "@stripe/stripe-react-native": "^0.57.2",
    "better-auth": "^1.4.15",
    "convex": "^1.31.5",
    "expo": "~55.0.0-canary-20251223-b83b31e",
    "expo-apple-authentication": "^8.0.8",
    "expo-application": "7.0.9-canary-20251223-b83b31e",
    "expo-blur": "^16.0.0-canary-20251230-fc48ddc",
    "expo-clipboard": "9.0.0-canary-20260105-6b962e6",
    "expo-constants": "~18.1.0-canary-20251223-b83b31e",
    "expo-dev-client": "^6.1.0-canary-20251223-b83b31e",
    "expo-device": "8.0.11-canary-20251223-b83b31e",
    "expo-font": "~14.1.0-canary-20251223-b83b31e",
    "expo-glass-effect": "0.2.0-canary-20251223-b83b31e",
    "expo-haptics": "~15.0.9-canary-20251223-b83b31e",
    "expo-image": "~3.1.0-canary-20251223-b83b31e",
    "expo-image-picker": "^17.1.0-canary-20251230-fc48ddc",
    "expo-linking": "~8.0.12-canary-20251223-b83b31e",
    "expo-mcp": "^0.2.3",
    "expo-network": "^8.0.9-canary-20251230-fc48ddc",
    "expo-notifications": "1.0.0-canary-20251223-b83b31e",
    "expo-router": "^7.0.0-canary-20260119-70f7c28",
    "expo-secure-store": "15.0.9-canary-20251223-b83b31e",
    "expo-splash-screen": "~31.0.14-canary-20251223-b83b31e",
    "expo-status-bar": "~3.0.10-canary-20251223-b83b31e",
    "expo-symbols": "~1.1.0-canary-20251223-b83b31e",
    "expo-system-ui": "~6.0.10-canary-20251223-b83b31e",
    "expo-task-manager": "14.0.10-canary-20251223-b83b31e",
    "expo-updates": "^29.1.0-canary-20251223-b83b31e",
    "expo-web-browser": "~15.0.11-canary-20251223-b83b31e",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-native": "0.83.1",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-keyboard-controller": "1.20.6",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "^4.20.0-nightly-20251223-914a4c101",
    "react-native-web": "~0.21.2",
    "react-native-webview": "^13.16.0",
    "react-native-worklets": "0.7.2"
  },
  "devDependencies": {
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.2.8",
    "eas-build-cache-provider": "^16.4.2",
    "eslint": "^9.39.2",
    "eslint-config-expo": "10.0.1-canary-20251223-b83b31e",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "expo-atlas": "^0.4.3",
    "jest": "^30.2.0",
    "jest-expo": "^55.0.0-canary-20251223-b83b31e",
    "patch-package": "^8.0.1",
    "react-test-renderer": "^19.2.3",
    "typescript": "~5.9.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react-native"
      ]
    }
  }
}
